import re

wiki = open('mosc.html', 'r', encoding = 'utf-8').read()
wiki = wiki.replace('\n',' ').replace('\t','')
rglat = 'latitude">.*?\&'
rglong = 'longitude">.*?\&'
lat = re.findall(rglat,wiki)
lat = re.findall('>.*',lat[0])
lat = lat[0][1:-2]
long = re.findall(rglong, wiki)
long = re.findall('>.*', long[0])
long = long[0][1:-2]
print(lat)
rglink = 'href="////tools.*?"'

print(long)
